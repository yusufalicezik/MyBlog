<% include ../partials/header %>
<div style="background-color:black;opacity:0.85 ;height:65px"></div>
<!-- Page Header -->
<div class="container" style="margin-top:80px;width:30%">
  <h1 style="text-align:center;">Sign in Form</h1>
  <h3 style="text-align:center;">Only for Admin</h3>
<form action="/signin" method="POST">
  <div class="form-group">
    <label for="emailAddress">Email address</label>
    <input type="email" class="form-control" id="emailAddress" aria-describedby="emailHelp" placeholder="Enter email" name="username"
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
    <label for="userPassword">Password</label>
    <input type="password" class="form-control" id="userPassword" placeholder="Password" name="password">
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>
<% include ../partials/footer %>
